  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:mock": "nuxt dev --dotenv .env-mock",
    "dev:api": "nuxt dev --dotenv .env-api",
    "generate": "nuxt generate",
    "generate:open-api-client": "npx openapi-typescript ./app/packages/http-client/girobet-backend-open-api.json -o app/packages/http-client/girobet-backend-generated-http-client/openapi-typescript.d.ts",
