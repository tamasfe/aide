    "lint:fix": "eslint . --fix",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "test": "vitest",
    "test:integration": "vitest --dir ./test/integration",
    "test:unit": "vitest --dir ./test/unit"
